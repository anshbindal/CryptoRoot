<canvas id="myChart" width="200" height="200"></canvas>

<script>
  const ctx = document.getElementById('myChart').getContext('2d'); const myChart
  = new Chart(ctx, { type: 'doughnut', data: { labels:
  {{{chartLabels}}}, datasets: [{ data:
  {{{chartData}}}, backgroundColor: [ 'rgb(255, 99, 132)', 'rgb(54, 162, 235)',
  'rgb(255, 205, 86)' ], hoverOffset: 4 }] } });
</script>

